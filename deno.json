{
  "tasks": {
    "dev": "deno serve --env-file=.env --port=9090 -A --watch main.ts",
    "test": "deno test -A --env-file=.env",
    "gen:client": "cd http && deno run -A npm:@hey-api/openapi-ts && deno fmt"
  },
  "imports": {
    "@/": "./",
    "@faker-js/faker": "npm:@faker-js/faker@^10.0.0",
    "@hono/standard-validator": "npm:@hono/standard-validator@^0.1.5",
    "@hono/swagger-ui": "npm:@hono/swagger-ui@^0.5.2",
    "@opentelemetry/api": "npm:@opentelemetry/api@^1.9.0",
    "@std/assert": "jsr:@std/assert@1",
    "@std/async": "jsr:@std/async@^1.0.15",
    "@std/expect": "jsr:@std/expect@1",
    "@std/testing": "jsr:@std/testing@^1.0.15",
    "@valibot/to-json-schema": "npm:@valibot/to-json-schema@^1.3.0",
    "hono": "npm:hono@^4.9.10",
    "hono-openapi": "npm:hono-openapi@^1.1.0",
    "ioredis": "npm:ioredis@^5.8.0",
    "mongodb": "npm:mongodb@^6.20.0",
    "mongoose": "npm:mongoose@^8.18.2",
    "valibot": "npm:valibot@^1.1.0"
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any", "require-await"]
    }
  }
}
